{
  "name": "redux-formula",
  "version": "0.1.0",
  "description": "An advanced selectors generator library",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage src",
    "build": "babel src/ -d lib/",
    "build-watch": "watch 'npm run build' ./src",
    "prepare": "npm run build && node post-build.js"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test)?(.verify).js?(x)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apendua/redux-formula.git"
  },
  "keywords": [
    "selector",
    "reselect",
    "redux"
  ],
  "author": "apendua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apendua/redux-formula/issues"
  },
  "homepage": "https://github.com/apendua/redux-formula#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "jest": "^22.4.2",
    "jsverify": "^0.8.3",
    "mkdirp": "^0.5.1",
    "npm-watch": "^0.3.0",
    "shortid": "^2.2.8"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.0-alpha.2"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "reselect": "^3.0.1"
  }
}
